{
  "name": "fire-boom",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "buildPrismaWorker": "npm run build --workspace src/lib/prisma",
    "buildSocketWorker": "npm run build --workspace src/lib/socket",
    "serve": "vite preview",
    "gen:api": "a2s generate -y",
    "lint": "pnpm lint:ts && pnpm lint:css",
    "lint:ts": "eslint ./src --fix",
    "lint:css": "stylelint ./src --fix --color",
    "intl:extract": "formatjs extract \"src/**/*.ts*\" --ignore=\"**/*.d.ts\" --out-file lang/zh-CN.json --id-interpolation-pattern [sha512:contenthash:base64:6]",
    "intl:compile": "formatjs compile-folder lang public/lang",
    "intl:zip": "cp lang/zh-CN.json lang/defaults.json && zip -r lang.zip lang",
    "intl": "npm run intl:extract && npm run intl:compile"
  },
  "dependencies": {
    "@ant-design/icons": "^5.2.6",
    "@antv/x6": "^1.35.0",
