    "@swordjs/monaco-editor-webpack-plugin": "^7.0.1",
    "@types/js-levenshtein": "1.1.2",
    "@uiw/react-json-view": "^1.12.1",
    "antd": "^5.9.4",
    "axios": "^1.2.2",
    "base64-js": "^1.5.1",
    "clsx": "^1.2.1",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
